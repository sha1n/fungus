    "jest-extended": "^1.0.0",
    "jest-html-reporters": "^3.0.4",
    "jest-summary-reporter": "^0.0.2",
    "prettier": "^2.4.1",
    "ts-jest": "^27.0.5",
    "uuid": "^8.3.2"
