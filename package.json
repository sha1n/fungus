  "license": "MIT",
  "types": "./dist/types",
  "main": "./dist",
  "exports": "./dist",
  "scripts": {
    "clean": "rm -rf ./dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "DEBUG=error:* jest --coverage",
